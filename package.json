    "@types/react-dom": "^19.1.7",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-client": "^3.0.0",
    "@types/ws": "^8.5.13",
    "concurrently": "^9.1.0",
    "eslint": "^9.33.0",
    "eslint-config-next": "15.4.6",
