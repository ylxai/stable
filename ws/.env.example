# WebSocket Server Configuration
NODE_ENV=production
WS_PORT=3001
HEALTH_PORT=3002
HOST=0.0.0.0

# Server Settings
MAX_CONNECTIONS=1000
HEARTBEAT_INTERVAL=30000
PING_TIMEOUT=120000

# Monitoring
ENABLE_STATS_LOGGING=true
STATS_INTERVAL=30000

# CORS Settings (untuk development)
CORS_ORIGIN=*

# Log Level
LOG_LEVEL=info